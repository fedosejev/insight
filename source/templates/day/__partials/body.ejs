<body class="page page-day">
  <div class="container">

    <%- include('../../__partials/header') %>

    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <div class="content-container">
        
          <h3 class="day">
            <a href="<%= websiteUrl %>/<%= year %>"><%= year %></a> 
            <a href="<%= websiteUrl %>/<%= year %>/<%= monthNumber %>"><%= monthName %></a> 
            <%= day %>, <%= weekday %>:
          </h3>

          <div class="content">
            <%- html %>
          </div>

        </div>
      </div>
    </div>

    <%- include('../../__partials/footer') %>

  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
  <script src="<%= websiteUrl %>/__static/js/app.js"></script>

  <%- include('../../__partials/google-analytics') %>
  <%- include('../../__partials/add-this') %>
</body>