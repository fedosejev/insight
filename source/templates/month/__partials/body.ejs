<body class="page page-month">
  <div class="container">

    <%- include('../../__partials/header') %>

    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <div class="content">
        
          <h3 class="month">
            <a href="<%= websiteUrl %>/<%= year %>"><%= year %></a> 
            <%= month.name %>
          </h3>

          <%

          let yearData = yearsMonthsDays[year];

          for (let iterator2 = 0; iterator2 < yearData.length; iterator2++) {

            if (iterator2 === parseInt(month.number, 10) && yearData[iterator2].length > 0) {

              let month = iterator2;
              let monthData = yearData[iterator2];

              for (let iterator3 = 0; iterator3 < monthData.length; iterator3++) {

                let day = monthData[iterator3];

              %>
                <h5 class="day">
                  <a href="<%= websiteUrl %>/<%= year %>/<%= month %>/<%= day.day %>">
                    <%= day.dayName %>
                  </a>
                </h5>
              <%
              }
            }
          }

          %>

        </div>
      </div>
    </div>    

    <%- include('../../__partials/footer') %>

  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
  <script src="/__static/js/app.js"></script>

  <%- include('../../__partials/google-analytics') %>
</body>